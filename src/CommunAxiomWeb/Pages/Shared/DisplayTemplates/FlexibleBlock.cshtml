@model CommunAxiomWeb.Extend.FlexibleBlock
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@if(Model.Tag.Id != "template"){
    @Html.Raw(Model.Opening)
    @for (var n = 0; n < Model.Items.Count; n++)
    {    
        @Html.DisplayFor(m => Model.Items[n], Model.Items[n].GetType().Name)
    }
    @Html.Raw(Model.Closing)
}
else
{
    @for (var n = 0; n < Model.Items.Count; n++)
    {    
        @Html.DisplayFor(m => Model.Items[n], Model.Items[n].GetType().Name)
    }
}